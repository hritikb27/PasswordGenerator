{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "react/no-unescaped-entities": 0,
    "@next/next/no-img-element": "off",
    "@next/next/no-sync-scripts": "warn",
    "prettier/prettier": [
      "error",
      {
        "useTabs": false
      },
      {
        "endOfLine": "auto"
      }
    ]
  }
}